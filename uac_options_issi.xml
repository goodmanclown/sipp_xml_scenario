<scenario name="Basic Sipstone UAC">
  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->
  <send retrans="500">
    <![CDATA[

      OPTIONS sip:TIA-P25-RFSSCapability@01.052.BEE20.p25dr;user=TIA-P25-RFSS SIP/2.0
      To: <sip:TIA-P25-RFSSCapability@01.052.BEE20.p25dr;user=TIA-P25-RFSS>
      From: <sip:TIA-P25-RFSSCapability@01.747.AB999.p25dr;user=TIA-P25-RFSS>;tag=[pid][call_number]
      Via: SIP/2.0/[transport] 01.747.AB999.p25dr:5060;branch=[branch]
      Call-ID: [call_id]
      Allow: INVITE,REGISTER,ACK,BYE,CANCEL,OPTIONS,MESSAGE
      CSeq: 1 OPTIONS
      Accept: application/sdp;level=1,application/x-tia-p25-issi,application/x-tia-p25-supdat,application/x-tia-p25-issi-90,multipart/mixed
      Max-Forwards: 70
      Content-Length: 0

    ]]>
  </send>

  <recv response="100"
        optional="true">
  </recv>

  <recv response="404" optional="true" id="5">
  </recv>

  <recv response="200" optional="true">
  </recv>

  <label id="5">
 
  <!-- definition of the response time repartition table (unit is ms)   -->
  <ResponseTimeRepartition value="10, 20, 30, 40, 50, 100, 150, 200"/>

  <!-- definition of the call length repartition table (unit is ms)     -->
  <CallLengthRepartition value="10, 50, 100, 500, 1000, 5000, 10000"/>

</scenario>

